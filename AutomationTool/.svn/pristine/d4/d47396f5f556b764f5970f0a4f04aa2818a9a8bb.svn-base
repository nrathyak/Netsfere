package com.infinite.netsfere.automationtool.modules;

public interface BasePage {

	// This method return true, if current page on device is this page
	public boolean isAt();

	// This method either navigates to this page or does nothing in case this
	// page is a specific item page (like conversation details page in which
	// this page is linked with a specific conversation)
	public void navigateTo() throws Exception;

	
}